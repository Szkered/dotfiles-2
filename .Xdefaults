!------------------------------------------------------------------!
! XFT                                                              !
!------------------------------------------------------------------!
Xft.dpi:                        96
Xft.autohint:                   0
Xft.antialias:                  1
Xft.rgba:                       rgb
Xft.lcdfilter:                  lcddefault
Xft.hinting:                    1
Xft.hintstyle:                  hintfull

Xcursor.theme:                  Neutral

!------------------------------------------------------------------!
! Emacs                                                            !
!------------------------------------------------------------------!

Emacs.pointerColor:             orange
!Emacs.FontBackend:             xft
!Emacs.geometry:                100x56+0+0
!Emacs.font:                    Inconsolata-13
!
!Multixterm*font:               xft:Arial:pixelsize=8

!------------------------------------------------------------------!
! Xterm                                                            !
!------------------------------------------------------------------!
!XTerm*faceName:                 DejaVu Sans Mono:size=12
XTerm*faceName:                 Proggy Clean:size=14
XTerm*termName:                 xterm-256color
XTerm*dynamicColors:            on
XTerm*allowSendEvents:          true
XTerm*utf8:                     2

!------------------------------------------------------------------!
! URxvt                                                            !
!------------------------------------------------------------------!
!URxvt*font:                     xft:DejaVu Sans Mono-9
!URxvt*boldFont:                 xft:DejaVu Sans Mono-9
!URxvt.talicFont:                xft:DejaVu Sans Mono-9
!URxvt.bolditalicFont:           xft:DejaVu Sans Mono-9
URxvt*font:                     xft:Ubuntu Mono-14
URxvt*boldFont:                 xft:Ubuntu Mono-14
URxvt.talicFont:                xft:Ubuntu Mono-14
URxvt.bolditalicFont:           xft:Ubuntu Mono-14

URxvt*letterSpace:              -1
Urxvt.depth:                    32
URxvt.colorDB:                  white
URxvt.colorIT:                  green
URxvt*termName:                 rxvt-unicode

URxvt.urgentOnBell:             True
URxvt*saveLines:                32767
URxvt*scrollBar:                false
URxvt*scrollBar_right:          false
URxvt*skipScroll:               false
URxvt*jumpScroll:               true
URxvt*loginShell:               true
!URxvt*shading:                  8
URxvt*fading:                   40
!URxvt*transparent:              true
!URxvt*inheritPixmap:            true
URxvt*secondaryScroll:          true
URxvt*visualBell:               true

! Perl extensions to rxvt
!URxvt.perl-ext:                 default,url-select,keyboard_select
URxvt.perl-ext-common:          default,keyboard-select,url-select

! URxvt.keysym.M-u:               perl:url-select:select_next

! URxvt.keysym.M-s:               perl:keyboard-select:search
! !URxvt.url-select.launcher:      /usr/bin/firefox -new-tab
! URxvt.url-select.underline:     true
! !
! URxvt.urlLauncher:      /usr/bin/firefox
! URxvt.matcher.button:   1
! URxvt.url-select.button:        1
! URxvt.matcher.pattern.1:        \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
! URxvt.matcher.pattern.2:        \\B(/\\S+?):(\\d+)(?=:|$)
! URxvt.matcher.launcher.2: gvim +$2 $1
! URxvt.matcher.launcher.2:  emacsclient -n -c $1

